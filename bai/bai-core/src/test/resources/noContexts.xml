<policySet xmlns="http://fuse.fusesource.org/schema/bai">
  <policy id="noContexts">
    <endpoints>
      <include>
        <endpoint>activemq:*</endpoint>
      </include>
    </endpoints>
    <events>
      <exclude>
        <event eventType="FAILURE_HANDLED"/>
      </exclude>
    </events>
  </policy>
</policySet>