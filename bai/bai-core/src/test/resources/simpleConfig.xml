<audit endpointUri="vm:audit"
       xmlns="http://fuse.fusesource.org/schema/bai">
  <policy id="nonAuditContexts">
    <contexts>
      <exclude>
        <context bundle="*" name="audit*"/>
      </exclude>
    </contexts>
    <events>
      <exclude>
        <event eventType="FAILURE_HANDLED"/>
      </exclude>
    </events>
    <endpoints>
      <include>
        <endpoint>seda:*</endpoint>
      </include>
    </endpoints>
  </policy>
</audit>