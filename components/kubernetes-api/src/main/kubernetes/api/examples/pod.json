{
  "kind": "Pod",
  "apiVersion": "v1beta1",
  "name": "php",
  "desiredState": {
    "manifest": {
      "version": "v1beta1",
      "id": "php",
      "containers": [
        {
          "name": "nginx",
          "image": "dockerfile/nginx",
          "ports": [
            {
              "containerPort": 80,
              "hostPort": 8080
            }
          ],
          "livenessProbe": {
            "initialDelaySeconds": 30,
            "httpGet": {
              "path": "/index.html",
              "port": {
                "IntVal": 8080
              }
            }
          }
        }
      ]
    }
  },
  "labels": {
    "name": "foo"
  }
}

