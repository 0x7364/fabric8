{
  "id": "fabric8MQController",
  "kind": "ReplicationController",
  "apiVersion": "v1beta1",
  "desiredState": {
    "replicas": 1,
    "replicaSelector": {
      "name": "fabricMQ",
      "container": "fabricMQ",
      "region": "default"
    },
    "podTemplate": {
      "desiredState": {
        "manifest": {
          "version": "v1beta1",
          "id": "fabric8MQController",
          "containers": [
            {
              "name": "fabric-mq",
              "image": "${docker.registry}/${docker.dataImage}",
              "ports": [
                {
                  "containerPort": 61616,
                  "hostPort": 61616
                },
                {
                  "containerPort": 8778,
                  "hostPort": 9801
                }
              ]
            }
          ]
        }
      },
      "labels": {
        "name": "fabricMQ",
        "container": "fabricMQ",
        "region": "default"
      }
    }
  },
  "labels": {
    "name": "fabricMQ",
    "container": "fabricMQ",
    "region": "default"
  }
}
