{
  "id": "fabric8MQConsumerController",
  "kind": "ReplicationController",
  "apiVersion": "v1beta1",
  "desiredState": {
    "replicas": 1,
    "replicaSelector": {
      "name": "fabricMQConsumer",
      "container": "java",
      "region": "default"
    },
    "podTemplate": {
      "desiredState": {
        "manifest": {
          "version": "v1beta1",
          "id": "fabric8MQConsumerController",
          "containers": [
            {
              "name": "fabric-mq-consumer",
              "image": "${docker.dataImage}",
              "ports": [
                {
                  "containerPort": 8778,
                  "hostPort": 9603
                }
              ],
              "env": [
                {
                  "name": "AMQ_PORT",
                  "value": "${activemq.host.port}"
                },
                {
                  "name": "AMQ_HOST",
                  "value": "dockerhost"
                }
              ]
            }
          ]
        }
      },
      "labels": {
        "name": "fabricMQConsumer",
        "container": "java",
        "region": "default"
      }
    }
  },
  "labels": {
    "name": "fabricMQConsumer",
    "container": "java",
    "region": "default"
  }
}
